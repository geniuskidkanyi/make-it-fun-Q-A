<% provide(:title, 'Make it fun Q&A - login') %>
<h1>login</h1>
<%= form_tag sessions_path, method: :post do%>
<p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <br/>
</p>
   <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br/>
  </p>
   <p>
    <%= submit_tag "Login" %>
  </p>
<% end %>
